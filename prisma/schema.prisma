datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String @id @default(uuid())
  username String @unique
  password String
}

model MyQuality {
  id          String @id @default(uuid())
  title       String
  desctiption String
}

model MyProject {
  id          String     @id @default(uuid())
  name        String
  description String
  imgUrl      String
  yearTo      Int
  yearFrom    Int
  skills      MySkills[]
}

model MyEducation {
  id          String @id @default(uuid())
  name        String
  description String
  yearTo      Int
  yearFrom    Int
}

model MySkills {
  id          String    @id @default(uuid())
  icon        String
  name        String
  myprojectId String
  myproject   MyProject @relation(fields: [myprojectId], references: [id])
}
